    "# solution 1\n",
    "def square_digits(n):\n",
    "    \"\"\"square each digit and return integer\"\"\"\n",
    "    num = ''\n",
    "    for x in str(n):\n",
    "        num += str(int(x)**2) \n",
    "    return int(num)"
